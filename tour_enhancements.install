<?php

/**
 * @file
 * Install, update and uninstall functions for the Tour Enhancements module.
 */

/**
 * Implements hook_install().
 */
function tour_enhancements_install() {
  \Drupal::messenger()->addStatus(__FUNCTION__);
}

/**
 * Implements hook_uninstall().
 */
function tour_enhancements_uninstall() {
  \Drupal::messenger()->addStatus(__FUNCTION__);
}
